{
  "name": "instagram-video-downloader-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@gravity-ui/stylelint-config": "^4.0.1",
    "cra-template-typescript": "1.2.0",
    "npm-run-all": "^4.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.3",
    "react-scripts": "5.0.1",
    "stylelint": "^15.8.0",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "run-p lint:js lint:styles lint:prettier typecheck",
    "lint:fix": "npm run lint:js:fix && npm run lint:styles:fix && npm run lint:prettier:fix && npm run typecheck",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}' --max-warnings=0",
    "lint:js:fix": "eslint '**/*.{js,jsx,ts,tsx}' --max-warnings=0 --fix",
    "lint:styles": "stylelint **/*.css",
    "lint:styles:fix": "stylelint **/*.css --fix",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx,css,scss,json,yaml,yml,md}'",
    "lint:prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,json,yaml,yml,md}'",
    "typecheck": "run-p typecheck:*",
    "typecheck:client": "tsc -p tsconfig.json --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@gravity-ui/eslint-config": "^3.3.0",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.57.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "prettier": "^3.4.2"
  }
}
